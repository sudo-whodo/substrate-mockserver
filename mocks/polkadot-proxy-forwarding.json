[
  {
    "httpRequest": {
      "method": "POST",
      "path": "/.*",
      "headers": {
        "Content-Type": ["application/json"]
      },
      "body": {
        "type": "JSON",
        "json": {
          "jsonrpc": "2.0"
        }
      }
    },
    "httpForward": {
      "host": "rpc.ibp.network",
      "port": 443,
      "scheme": "HTTPS"
    }
  },
  {
    "httpRequest": {
      "method": "GET",
      "path": "/.*"
    },
    "httpForward": {
      "host": "rpc.ibp.network",
      "port": 443,
      "scheme": "HTTPS"
    }
  }
]
